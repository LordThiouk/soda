# Environnement d'exécution (development, production, test)
NODE_ENV=development

# Port sur lequel le serveur écoute
PORT=5000

# Configuration Supabase
SUPABASE_URL=https://your-project-url.supabase.co
SUPABASE_KEY=your-supabase-service-role-key

# Secret pour la génération des JWT
JWT_SECRET=your-secret-key-min-32-chars

# Durée de validité des JWT (en secondes)
JWT_EXPIRES_IN=86400

# Configuration CORS
CORS_ORIGIN=http://localhost:3000

# Configuration des téléchargements
UPLOAD_FILE_SIZE_LIMIT=10485760  # 10MB en octets
UPLOAD_DIR=uploads

# Configuration des services de reconnaissance musicale
ACOUSTID_API_KEY=your-acoustid-api-key
AUDD_API_KEY=your-audd-api-key

# Configuration des logs
LOG_LEVEL=info  # Options: error, warn, info, http, debug

# Configuration du rate limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes en millisecondes
RATE_LIMIT_MAX=100           # Nombre maximum de requêtes par fenêtre 